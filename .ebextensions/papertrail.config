files:
    "/etc/rsyslog.conf":
    mode: "000644"
    owner: ec2-user
    group: ec2-user
    source: "https://s3-us-west-2.amazonaws.com/ac-resources/rsyslog.conf"

services:
    sysvinit:
        rsyslog:
            enabled: true
            ensureRunning: true
            files: "/etc/rsyslog.conf"
